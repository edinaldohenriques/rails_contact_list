<div class="row">
  <div class="col-xs-12">
    <h1>Contato</h1>
  </div>

  <div class="col-md-6 col-md-offset-3 col-xs-12">
    <%= form_for @contact do |f| %>
      <%= render 'form', f: f %>


      <%= link_to 'Editar', edit_contact_path(@contact), class: 'btn btn-danger' %> |
      <%= link_to 'Voltar', contacts_path, class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
